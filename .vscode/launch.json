{
    "version": "0.2.0",
    "configurations": [
      {
        "name": "Python: Current File",
        "type": "python",
        "request": "launch",
        "program": "${file}",
        "console": "integratedTerminal",
        "env": {
          "AIRFLOW_HOME": "${workspaceFolder}/airflow"
        },
        "envFile": "${workspaceFolder}/airflow/.env",
      }
    ]
  }
  